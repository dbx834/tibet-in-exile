{
  "name": "tibet-in-exile",
  "private": true,
  "version": "0.0.1",
  "description": "App for Tibe in Exile",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dbx834/tibet-in-exile.git"
  },
  "keywords": [
    "meteor"
  ],
  "author": "dbx834",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dbx834/tibet-in-exile/issues"
  },
  "homepage": "https://github.com/dbx834/tibet-in-exile#readme",
  "scripts": {
    "start": "meteor run",
    "lint": "eslint . --ext .js --ext .jsx --ignore ./node_modules",
    "pretest": "npm run lint --silent"
  },
  "eslintConfig": {
    "plugins": [
      "meteor"
    ],
    "extends": [
      "airbnb",
      "plugin:meteor/recommended"
    ],
    "rules": {
      "no-nested-ternary": 0,
      "func-names": 0,
      "consistent-return": 0,
      "max-len": 0,
      "padded-blocks": 0,
      "prefer-arrow-callback": 0,
      "no-trailing-spaces": [
        "error",
        {
          "skipBlankLines": true
        }
      ],
      "no-underscore-dangle": [
        "error",
        {
          "allow": [
            "_id"
          ]
        }
      ],
      "arrow-body-style": [
        "error",
        "always"
      ],
      "valid-jsdoc": "error",
      "require-jsdoc": [
        "error",
        {
          "require": {
            "FunctionDeclaration": true,
            "MethodDefinition": true,
            "ClassDeclaration": true
          }
        }
      ],
      "meteor/eventmap-params": [
        2,
        {
          "templateInstanceParamName": "instance"
        }
      ],
      "import/no-unresolved": [
        2,
        {
          "ignore": [
            "^meteor/"
          ]
        }
      ]
    },
    "globals": {
      "Meteor": true,
      "Mongo": true,
      "Package": true,
      "Npm": true
    }
  },
  "dependencies": {
    "antd": "^1.6.5",
    "classnames": "^2.2.5",
    "jss": "^5.2.0",
    "jss-camel-case": "^1.1.0",
    "jss-nested": "^1.0.3",
    "jss-px": "^1.0.0",
    "jss-vendor-prefixer": "^2.0.0",
    "lodash": "^4.14.0",
    "meteor-node-stubs": "~0.2.3",
    "moment": "^2.14.1",
    "react": "^15.3.0",
    "react-addons-pure-render-mixin": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-jss": "^2.1.1",
    "react-motion": "^0.4.4",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.0",
    "react-router-redux": "^4.0.5",
    "react-simple-parallax": "^0.2.6",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "tracker-component": "^1.3.21",
    "typography": "^0.13.0",
    "typography-theme-kirkham": "^0.13.0"
  },
  "devDependencies": {
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-meteor": "^3.6.0",
    "eslint-plugin-react": "^5.2.2",
    "eslint-plugin-redux": "^0.1.0",
    "redux-devtools": "^3.3.1"
  }
}
